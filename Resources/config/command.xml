<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <defaults autowire="true" public="false">
            <bind key="$executor" id="f_devs_fixture.executor_iterator"/>
        </defaults>

        <service class="FDevs\Fixture\Command\LoadCommand">
            <argument key="$executor" id="f_devs_fixture.executor"/>
        </service>

        <service id="f_devs_fixture.executor" class="FDevs\Executor\Executor">
            <argument key="$iterator" id="f_devs_fixture.executor_iterator"/>
        </service>

        <service id="f_devs_fixture.executor_iterator"
                 class="FDevs\Executor\DependentExecutableIterator">
            <argument type="tagged" tag="f_devs_fixture.fixture"/>
        </service>

    </services>

</container>