<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <defaults autowire="true" public="false">
            <bind key="$executor" id="f_devs_fixture.executor_iterator"/>
        </defaults>

        <service id="FDevs\Fixture\Command\LoadCommand">
            <tag name="console.command" command="fdevs:fixture:load"/>
        </service>

        <service id="FDevs\Executor\ExecutorInterface" class="FDevs\Executor\Executor"/>

        <service id="FDevs\Executor\DependentExecutableIteratorInterface"
                 alias="FDevs\Executor\DependentExecutableIterator"/>
